{
  "name": "extalion/sentry",
  "description": "Sentry integration with prestashop",
  "type": "prestashop-module",
  "autoload": {
    "psr-4": {
      "Extalion\\Sentry\\": "src/"
    }
  },
  "config": {
    "prepend-autoloader": false
  },
  "require-dev": {
    "phpmd/phpmd": "^2.10",
    "prestashop/php-dev-tools": "^4.2",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "require": {
    "sentry/sdk": "^3.3"
  }
}
